<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
  <div class="card" style="width: 18rem;">
  <%= image_tag @blog.main_photo %>
  <div class="card-body">
    <h5 class="card-title"><%= @blog.title %></h5>
    <p class="card-text"><%= @blog.body %></p>
    <%= link_to 'Delete', blog_path(@blog), method: :delete, data: { confirm: "Are you sure you want to remove this?"} %>
    <%= link_to 'Edit', edit_blog_path(@blog) %>
  </div>
</div>
<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <%= image_tag @blog.photo_one unless @blog.photo_one.blank? %>
    </div>
    <div class="carousel-item">
      <%= image_tag @blog.photo_two unless @blog.photo_two.blank? %>
    </div>
    <div class="carousel-item">
      <%= image_tag @blog.photo_three unless @blog.photo_three.blank? %>
    </div>
        <div class="carousel-item">
      <%= image_tag @blog.photo_four unless @blog.photo_four.blank? %>
    </div>
        <div class="carousel-item">
      <%= image_tag @blog.photo_five unless @blog.photo_five.blank? %>
    </div>
        <div class="carousel-item">
      <%= image_tag @blog.photo_six unless @blog.photo_six.blank? %>
    </div>
        <div class="carousel-item">
      <%= image_tag @blog.photo_seven unless @blog.photo_seven.blank? %>
    </div>
        <div class="carousel-item">
      <%= image_tag @blog.photo_eight unless @blog.photo_eight.blank? %>
    </div>
        <div class="carousel-item">
      <%= image_tag @blog.photo_nine unless @blog.photo_nine.blank? %>
    </div>
        <div class="carousel-item">
      <%= image_tag @blog.photo_ten unless @blog.photo_ten.blank? %>
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

</body>
</html>




